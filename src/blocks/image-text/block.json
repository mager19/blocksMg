{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blocksmg/image-text",
	"version": "0.1.0",
	"title": "BMG-image-text",
	"category": "blocksmg",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"image": {
			"type": "object",
			"default": {
			"url" : "https://via.placeholder.com/644?text=Image",
			"alt" : "placeholder"
			}
		},
		"imageToRight": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"align": true
	},
	"keywords": [
		"image",
		"blocksmg",
		"text"
	],
	"textdomain": "blocksmg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./markup.php",
	"viewScript": "file:./view.js"
}
